/* 中秋图片 */

.zhongqiuPic {
  width: 83rpx;
  height: 147rpx;
  background: url('http://oibl5dyji.bkt.clouddn.com/zqscoll.png') no-repeat;
   background-size: 415rpx 147rpx;  
  position: absolute;
  top: 0;
  right: 38rpx;
  z-index: 1001;
  animation: changeZq 2.5s steps(1) infinite;
}

@keyframes changeZq {
  0% {
    background-position: 0 0;
   
  }
 
  25% {
    background-position: -83rpx 0;
   
  }

  50% {
    background-position: -165rpx 0;
  
  }

  75% {
    background-position: -249rpx 0;
   
  }

  100% {
    background-position: -332rpx 0;
   
  }
}

