page{
  width: 100%;
  height: 100%;
  background:#f8f8f8;
}
.outbox{
  position: relative;
  width: 100%;
  height: 520rpx;
}
.outbox image{
  width: 320rpx;
  height: 320rpx;
  position: absolute;
  top:130rpx;
  left: 50%;
  transform: translateX(-50%);
}
.photoinfo view:nth-child(1){
  display: flex;
  flex-direction: column;
  text-align: center;
  font-family:PingFangSC-Regular;
  font-size:48rpx;
  color:#333333;
  letter-spacing:0;
  margin-top: 0rpx;
}

.photoinfo view:nth-child(2){
text-align: center;
font-family:PingFangSC-Regular;
font-size:24rpx;
color:#666666;
letter-spacing:0;
margin-top: 34rpx;

}
.code{
  display: flex;
  padding: 76rpx 0rpx;
flex-flow: row wrap;
justify-content: center;  
}
.code view{
  background:#ffffff;
  width:72rpx;
  height:80rpx;
  margin-right: 8rpx;
  line-height: 80rpx;
  text-align: center;
}
.code view text{
font-family:PingFang-SC-Medium;
font-size:48rpx;
color:#333333;
letter-spacing:0;
text-align:center;
}
.successtip{
  font-family:PingFangSC-Regular;
font-size:16px;
color:#a0a0a0;
letter-spacing:0;
text-align:center;
}
.gotobook{
  background:#fff04a;
border-radius:16rpx;
width:494rpx;
height:98rpx;
text-align: center;
margin: 0 auto;
font-family:PingFangSC-Regular;
font-size:32rpx;
color:#333333;
letter-spacing:0;
line-height: 98rpx;
margin-top: 328rpx;

}

